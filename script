var leftMargin = 1;

var moveAmount = 10;
function boxClicked() {
    console.log("Hey, box clicked!");



    if (leftMargin < 100) {
        leftMargin += moveAmount;

    }
    leftMargin += moveAmount;


    boxes[1].style.marginLeft = leftMargin + "px";

}

boxes[0].addEventListener("click", boxClicked);
console.log(boxes[1]);